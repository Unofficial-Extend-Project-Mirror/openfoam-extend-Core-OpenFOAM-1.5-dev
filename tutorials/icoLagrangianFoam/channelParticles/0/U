{
    in
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }

    out
    {
        type            zeroGradient;
    }

    topAndBottom      
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    frontAndBack    
    {
        type            empty;
    }
}


