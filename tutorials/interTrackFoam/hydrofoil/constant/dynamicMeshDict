FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          meshMotionDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

twoDMotion          yes;

solver              laplaceFaceDecomposition;

diffusivity         patchEnhanced;

distancePatches     1 (freeSurface);

frozenDiffusion     yes;


pseudoSolid
{
    poissonsRatio           0.3;
    nCorrectors             3;
    convergenceTolerance    1e-9;
};

