6
(
left
{
    type symmetryPlane;
    nFaces 30;
    startFace 1930;
}

right
{
    type patch;
    nFaces 30;
    startFace 1960;
}

down
{
    type symmetryPlane;
    nFaces 30;
    startFace 1990;
