FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSolution;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    phi PCG
    {
        preconditioner
        {
            type        DIC;
        }

        minIter          0;
        maxIter          2000;
        tolerance        1e-08;
        relTol           0.2;
    };
    rho PBiCG
    {
        preconditioner
        {
            type        DILU;
        }

        minIter          0;
        maxIter          500;
        tolerance        1e-08;
        relTol           0.2;
    };
}


