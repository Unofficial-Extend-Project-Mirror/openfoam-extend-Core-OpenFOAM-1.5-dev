FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSolution;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p               ICCG 1e-06 0.01;
    U               BICCG 1e-05 0.1;
    k               BICCG 1e-05 0.1;
    epsilon         BICCG 1e-05 0.1;
    R               BICCG 1e-05 0.1;
    nuTilda         BICCG 1e-05 0.1;
}

PISO
{
    nCorrectors     10;
    nNonOrthogonalCorrectors 0;
}

relaxationFactors
{
    p               0.5;
    U               0.9;
    k               0.9;
    epsilon         0.9;
    R               0.9;
    nuTilda         0.9;
}


